name: dockercompose4601289286035636859
services:
  rms.web:
    build:
      context: C:\MVC_Project\RMS-mvc
      dockerfile: RMS.Web/Dockerfile
    container_name: rms-web
    depends_on:
      rmsdb:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Production
      ASPNETCORE_URLS: http://+:80
      DB_HOST: rmsdb
      DB_NAME: RMS
      DB_SA_PASSWORD: ""
    image: rmsweb:latest
    networks:
      rms_network: null
    ports:
      - mode: ingress
        target: 80
        published: "8001"
        protocol: tcp
      - mode: ingress
        target: 80
        published: "80"
        protocol: tcp
    restart: always
  rmsdb:
    container_name: rms-db
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Your_password123!
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      rms_network: null
    ports:
      - mode: ingress
        target: 1433
        published: "8002"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: rms_data
        target: /var/opt/mssql
        volume: {}
networks:
  rms_network:
    name: dockercompose4601289286035636859_rms_network
    driver: bridge
volumes:
  rms_data:
    name: dockercompose4601289286035636859_rms_data